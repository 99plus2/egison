(define $f
  (lambda [$n]
    (foldl + 0 (map (power $ 2) (map (compose (lambda [$n] {n}) read) (show n))))))

(define $d (generate-array [$i] [567] (p-naive i)))

(define $p-naive
  (lambda [$n]
    (match n integer
      {[,1 #f]
       [,89 #t]
       [_ (p-naive (f n))]})))

(define $p
  (lambda [$n]
    (match n integer
      {[,1 #f]
       [,89 #t]
       [_ (array-ref d (f n))]})))

