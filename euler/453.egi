(define $nperm
  (lambda [$n $r]
    (foldl * 1 (between (- n (- r 1)) n))))

(define $ncomb
  (lambda [$n $r]
    (/ (nperm n r)
       (fact r))))

(define $x 12345)
(define $y 6789)

(test (* (+ x 1) (+ y 1)))

(test (ncomb (* (+ x 1) (+ y 1)) 2))

(define $intersect?
  (lambda [$x11 $y11 $x12 $y12 $x21 $y21 $x22 $y22]
    undefined))